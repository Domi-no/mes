.step-wrap{
  height: 380rpx;
  position: relative;
}
.step-wrap image{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
}
.step-wrap .sw{
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 2;
  width: 90%;
  height: 240rpx;
  transform: translate(-50%, -50%);
}
.info-wrap{
  position: relative;
  z-index: 3;
  padding: 0 30rpx 30rpx;
  margin: -140rpx auto 0;
}
.common-wrap{
  background-color: #fff;
  margin-bottom: 30rpx;
}
.common-wrap:last-child{
  margin-bottom: 0;
}
.common-wrap .title{
  padding:0 30rpx;
  margin-bottom: 30rpx;
}
.common-wrap .title.cmi{
  margin-bottom: 0;
}
.common-wrap .title text{
  display: block;
  width: 100%;
  line-height: 97rpx;
  padding-left: 30rpx;
  font-size: 34rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
  border-bottom: 1px solid #E8EBF3;
  position: relative;
}
.common-wrap .title text::before{
  content: '';
  position: absolute;
  top: 35rpx;
  left: 0rpx;
  width: 10rpx;
  height: 34rpx;
  background: linear-gradient(180deg,#00b3ff, #075fff);
}

.common-wrap .row .label,
.common-wrap .row .value
{
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
}
.common-wrap .row .label{
  color: #868BA2;
  font-weight: 400;
}
.common-wrap .row .value{
  flex: 1;
  color: #000000;
  font-weight: 500;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.common-wrap .row{
  display: flex;
  align-items: center;
  padding: 0 30rpx 30rpx;
}
.materials-list{
  padding: 0 30rpx 30rpx;
}
.materials-list .item{
  background: #f6f7fb;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
}
.materials-list .item:last-child{
  margin-bottom: 0;
}
.materials-list .item .i-header{
  height: 89rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #E8EBF3;
}
.materials-list .item .i-header .left-text{
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
}
.materials-list .item .i-header .right-text{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
}
.materials-list .item .i-body{
  padding: 30rpx 0;
  display: flex;
  align-items: center;
}
.materials-list .item .i-body .image,
.modal-wrapper .mwc-consume .i-body .image{
  width: 190rpx;
  height: 190rpx;
  margin-right: 30rpx;
}
.materials-list .item .i-body .ib-row,
.modal-wrapper .mwc-consume .i-body .ib-row{
  display: flex;
  align-items: center;
  margin-bottom: 14rpx;
}
.materials-list .item .i-body .ib-row:last-child,
.modal-wrapper .mwc-consume .i-body .ib-row:last-child{
  margin-bottom: 0;
}
.materials-list .item .i-body .label,
.materials-list .item .i-body .value,
.modal-wrapper .mwc-consume .i-body .label,
.modal-wrapper .mwc-consume .i-body .value
{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
}
.materials-list .item .i-body .label,
.modal-wrapper .mwc-consume .i-body .label{
  color: #868BA2;
  font-weight: 400;
}
.materials-list .item .i-body .value,
.modal-wrapper .mwc-consume .i-body .value{
  flex: 1;
  color: #000000;
  font-weight: 500;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.materials-list .item .i-footer{
  height: 124rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  border-top: 1px solid #E8EBF3;
}
.quality-table{
  padding: 0 30rpx 30rpx;
}
.quality-table .t1{
  background: #f6f7fb;
}
.quality-table .t2{
  border: 2rpx solid #e8ebf3;
}
.quality-table .t-wrap{
  margin-bottom: 30rpx;
}
.quality-table .t-wrap:last-child{
  margin-bottom: 0;
}
.quality-table .t-wrap .t-header{
  width: 570rpx;
  height: 89rpx;
  margin: auto;
  border-bottom: 1px solid #E8EBF3;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.quality-table .t2 .t-header{
  width: 570rpx;
  height: 89rpx;
  margin: auto;
  border-bottom: 1px solid #E8EBF3;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #000000;
}
.quality-table .t-wrap .t-header .left-text{
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
}
.quality-table .t-wrap .t-header .right-text{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
}
.quality-table .t-wrap .t-body{
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.quality-table .t-wrap .t-body .tb-column{
  width: 33.33%;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.quality-table .t-wrap .t-body .tb-column .label{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
}
.quality-table .t-wrap .t-body .tb-column .value{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #000000;
  margin-bottom: 10rpx;
}
.quality-table .t-wrap .t-body .tb-column:nth-child(2){
  position: relative;
}
.quality-table .t-wrap .t-body .tb-column:nth-child(2)::before{
  content: '';
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 1px;
  height: 84rpx;
  background-color: #E8EBF3;
}
.quality-table .t-wrap .t-body .tb-column:nth-child(2)::after{
  content: '';
  position: absolute;
  top: 20rpx;
  right: 0;
  width: 1px;
  height: 84rpx;
  background-color: #E8EBF3;
}
.complate-input{
  width: 570rpx;
  height: 120rpx;
  margin: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.complate-input .label{
  font-size: 30rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #000000;
  margin-right: 20rpx;
}
.complate-input input{
  display: block;
  border: none;
  outline: none;
  flex: 1;
  text-align: right;
}
.fill-in-empty{
  height: 130rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: content-box;
}
.fixed-footer{
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 5;
  width: 100%;
  height: 130rpx;
  box-shadow: 0rpx 16rpx 40rpx 0rpx rgba(0,0,0,0.24); 
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: content-box;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
/* 领取、还回耗材弹窗 */
.modal-wrapper .mwc-expend{
  width: 100%;
  margin: 50rpx 0;
}
.modal-wrapper .mwc-expend .input-row{
  display: flex;
  flex-direction: column;
}
.modal-wrapper .mwc-expend .label{
  font-size: 30rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
  margin-bottom: 30rpx;
}
/* 公用输入框 */
.modal-wrapper .mw-inner .content input{
  display: block;
  flex: 1;
  height: 88rpx;
  background: #F6F7FB;
  border: 1rpx solid #e8ebf3;
  padding: 0 10rpx;
  box-sizing: border-box;
}
/* 质检信息弹窗 */
.modal-wrapper .mwc-quality{
  width: 100%;
  overflow: hidden;
}
.modal-wrapper .mwc-quality .names,
.modal-wrapper .mwc-consume .names{
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
  margin: 50rpx auto 30rpx;
}
.modal-wrapper .mwc-quality .input-row,
.modal-wrapper .mwc-consume .input-row,
.modal-wrapper .mwc-production .input-row{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.modal-wrapper .mwc-quality .input-row:last-child{
  margin-bottom: 50rpx;
}
.modal-wrapper .mwc-quality .label,
.modal-wrapper .mwc-consume .label,
.modal-wrapper .mwc-production .label{
  font-size: 30rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
  margin-right: 30rpx;
}
/* 耗材查验弹窗 */
.modal-wrapper .mwc-consume{
  width: 100%;
  max-height: 800rpx;
  overflow-y: scroll;
}
.modal-wrapper .mwc-consume .i-body{
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #F6F7FB;
  padding: 20rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}
/* 生成订单扫码进入弹窗 */
.modal-wrapper .mwc-production .content{
  width: 100%;
}
.modal-wrapper .mwc-production .i-body{
  padding: 40rpx 0;
  border-bottom: 1px solid #E8EBF3;
  margin-bottom: 30rpx;
}
.modal-wrapper .mwc-production .ib-row{
  display: flex;
  align-items: center;
  line-height: 66rpx;
}
.modal-wrapper .mwc-production .ibr-label,
.modal-wrapper .mwc-production .ibr-value{
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
}
.modal-wrapper .mwc-production .ibr-label{
  color: #868ba2;
}
.modal-wrapper .mwc-production .ibr-value{
  color: #000000;
}
.modal-wrapper .mwc-production .input-row{
  margin-bottom: 50rpx;
}
.modal-wrapper .mwc-production .p-btn{
  width: 164rpx;
  height: 80rpx;
}
.modal-wrapper .mwc-production .p-btn.shift-btn{
  background-color: #07CAFF;
  color: #fff;
}

/* 详情底栏按钮组 */
.bottom-fixed-btn{
  width:330rpx;
  height:86rpx
}
.bottom-fixed-btn-v2{
  width:170rpx;
  height:86rpx
}