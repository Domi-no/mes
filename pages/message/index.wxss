page{
  background: #fff;
}
.msg-list .item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 25rpx 30rpx;
}
.msg-list .item .icon{
  width: 100rpx;
  height: 100rpx;
  margin-right: 30rpx;
}
.msg-list .item .right{
  flex: 1;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #e8ebf3;
}
.msg-list .item .right .rt{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.msg-list .item .right .title{
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #000000;
}
.msg-list .item .right .time{
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #c3c7d4;
}
.msg-list .item .right .label{
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #868ba2;
}
