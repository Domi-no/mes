.head{
  position: relative;
  width: 100%;
  height: 300rpx;
  margin: 0;
  padding: 0;
}
.img{
  position: absolute;
  width: 690rpx;
  height: 240rpx;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  z-index: -1;
}
.head-img{
  position: absolute;
  height:200rpx;
  margin:50rpx 80rpx;
  display:flex;
  align-items: center;
}
.left-img{
  width:140rpx;
  height:140rpx;
}
.right{
  margin-left:36rpx;
  color:#fff;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
}
.names{
  display: flex;
  align-items: center;
  justify-content: center;
}
.username{
  font-size: 48rpx;
  line-height: 64rpx;
}
.types{
  font-size: 22rpx;
  color: #075fff;
  display:inline-block;
  line-height: 44rpx;
  width: 118rpx;
  height: 44rpx;
  background: #ffffff;
  border-radius: 22rpx;
  text-align: center;
  margin-left:20rpx;
}
.factory{
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #ffffff;
  line-height: 40rpx;
  margin-top:8rpx;
}
.top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 140rpx;
  width:690rpx;
  margin:0 auto;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
}
.content{
  display: flex;
  align-items: center;
}
.iconts{
  width: 46rpx;
  height: 46rpx;
  margin-right:32rpx;
}
.iconts2{
  width:18rpx;
  height:32rpx;
}
.line{
  width:100%;
  height:16rpx;
  background-color: #f6f7fb;
}
